{
  "name": "incent-api",
  "version": "1.0.0",
  "description": "IncentCard API Server",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run serve",
    "serve": "node dist/server.js",
    "build": "npm run build-ts",
    "build-ts": "tsc",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com//IncentCard/incent-api"
  },
  "author": "IncentCard Team",
  "license": "",
  "dependencies": {
    "@types/express": "^4.11.1",
    "@types/uuid": "^3.4.3",
    "express": "^4.16.3",
    "typescript": "^2.8.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.3",
    "tslint": "^5.9.1"
  }
}
