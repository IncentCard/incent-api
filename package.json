{
  "name": "incent-api",
  "version": "1.0.0",
  "description": "IncentCard API Server",
  "author": "IncentCard Team",
  "license": "",
  "main": "dist/app/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "npm run test --watch",
    "start": "npm run build && ts-node -r dotenv/config dist/app/server.js",
    "build": "tsc",
    "clean": "rimraf dist/*",
    "clean:build": "npm run clean && npm run build",
    "watch": "nodemon --config nodemon.json",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase functions:shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com//IncentCard/incent-api"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "firebase-admin": "^5.12.1",
    "firebase-functions": "^1.1.0",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "^7.4.3",
    "request": "^2.87.0",
    "typescript-logging": "^0.6.2",
    "uuid": "^3.3.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^2.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^22.2.3",
    "@types/jest-each": "^0.3.1",
    "@types/passport": "^0.4.5",
    "@types/passport-http": "^0.3.6",
    "@types/passport-http-bearer": "^1.0.33",
    "@types/pg": "^7.4.10",
    "@types/request": "^2.47.1",
    "@types/sinon": "^4.3.3",
    "@types/supertest": "^2.0.4",
    "@types/uuid": "^3.4.3",
    "concurrently": "^3.6.0",
    "jest": "^22.4.4",
    "jest-each": "^0.5.0",
    "nodemon": "^1.17.5",
    "rimraf": "^2.6.2",
    "sinon": "^5.1.1",
    "supertest": "^3.1.0",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.2.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  }
}
